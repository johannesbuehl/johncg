{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"semi": ["error", "always"],
		"object-curly-spacing": ["error", "always"],
		"quotes": ["error"],
		"quote-props": ["error", "as-needed"]
	}
}